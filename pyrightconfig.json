{
  "include": ["src", "tests"],
  "exclude": [
    "**/__pycache__",
    "**/.pytest_cache",
    "**/.git",
    "outputs",
    "cache",
    "data",
    "database",
    "FishBroData",
    ".venv"
  ],
  "pythonVersion": "3.10",
  "pythonPlatform": "Linux",
  "venvPath": ".",
  "venv": ".venv",
  "executionEnvironments": [
    { "root": "src", "extraPaths": ["src"] },
    { "root": "tests", "extraPaths": ["src"] }
  ],
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "reportPrivateImportUsage": false
}
